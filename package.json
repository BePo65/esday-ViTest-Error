{
  "name": "esday-ViTest-Error",
  "type": "module",
  "version": "0.4.1",
  "packageManager": "pnpm@10.6.3",
  "description": "A minimal project to reproduce vitest error of esday project",
  "license": "MIT",
  "homepage": "https://github.com/BePo65/esday-ViTest-Error",
  "repository": {
    "type": "git",
    "url": "https://github.com/BePo65/esday-ViTest-Error"
  },
  "bugs": {
    "url": "https://github.com/BePo65/eesday-ViTest-Errorsday/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./*": {
      "types": "./*/index.d.ts",
      "default": "./*/index.js"
    }
  },
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "typesVersions": {},
  "files": [
    "*"
  ],
  "scripts": {
    "build": "tsup --dts",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test": "vitest",
    "test:ui": "vitest --ui --api 9527",
    "test:run": "vitest --run",
    "test:run-browser": "vitest --run --browser.enabled --testTimeout=20000 --reporter=verbose",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@playwright/test": "~1.51.0",
    "@types/node": "^22.13.10",
    "@vitest/browser": "3.0.8",
    "@vitest/coverage-v8": "3.0.8",
    "@vitest/ui": "3.0.8",
    "moment": "^2.30.1",
    "playwright": "~1.51.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vitest": "v3.0.8"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild",
      "msw"
    ],
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
