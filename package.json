{
  "name": "esday-ViTest-Error",
  "type": "module",
  "version": "0.4.1",
  "packageManager": "pnpm@9.15.2",
  "description": "A minimal project to reproduce vitest error of esday project",
  "license": "MIT",
  "homepage": "https://github.com/BePo65/esday-ViTest-Error",
  "repository": {
    "type": "git",
    "url": "https://github.com/BePo65/esday-ViTest-Error"
  },
  "bugs": {
    "url": "https://github.com/BePo65/eesday-ViTest-Errorsday/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./*": {
      "types": "./*/index.d.ts",
      "default": "./*/index.js"
    }
  },
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "typesVersions": {},
  "files": [
    "*"
  ],
  "scripts": {
    "build": "tsup --dts",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test": "vitest",
    "test:ui": "vitest --ui --api 9527",
    "test:run": "vitest --run",
    "test:run-browser": "vitest --run --browser.enabled --testTimeout=20000 --reporter=verbose",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@playwright/test": "~1.49.1",
    "@types/node": "^22.13.4",
    "@vitest/browser": "3.0.5",
    "@vitest/coverage-v8": "3.0.5",
    "@vitest/ui": "3.0.5",
    "moment": "^2.30.1",
    "playwright": "~1.49.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "v3.0.5"
  }
}
